{
  "name": "uttori-storage-provider-json-file",
  "version": "2.2.2",
  "description": "Uttori storage provider using JSON files on disk.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": "uttori/uttori-storage-provider-json-file",
  "homepage": "https://github.com/uttori/uttori-storage-provider-json-file",
  "keywords": [
    "uttori",
    "json"
  ],
  "engines": {
    "node": ">= 10.15.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fs-extra": "^8.1.0",
    "ramda": "^0.26.1",
    "uttori-utilities": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "coveralls": "^3.0.6",
    "docsify-cli": "^4.3.0",
    "eslint": "^5",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "eslint-plugin-xss": "^0.1.9",
    "jsdoc-to-markdown": "^5.0.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "supertest": "^4.0.2",
    "uttori-document": "^1.4.0"
  },
  "files": [
    "src/"
  ],
  "main": "./src/index.js",
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": true,
    "include": [
      "src/"
    ],
    "exclude": []
  },
  "scripts": {
    "docs": "jsdoc2md --private --example-lang js --template README.hbs src/index.js > README.md; cp README.md docs/README.md",
    "docs-serve": "docsify serve ./docs",
    "lint": "eslint .",
    "report": "nyc report --reporter=html",
    "test": "NODE_ENV=test nyc ava --serial --concurrency=1",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test nyc ava --serial --concurrency=1",
    "validate": "npm ls"
  }
}
